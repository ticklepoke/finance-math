{
  "name": "finance-math",
  "version": "1.0.3",
  "description": "Financial Math for JS",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/bundle.min.js",
  "types": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "docs": "typedoc",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "build": "run-p compile:* bundle",
    "compile": "babel src --extensions '.ts' --ignore '**/*.test.ts'",
    "compile:cjs": "npm run compile -- --out-dir dist",
    "compile:esm": "BABEL_ENV=esm npm run compile -- --out-dir dist/esm",
    "bundle": "rollup -c",
    "postbuild": "run-p copy:*",
    "copy": "rsync -zarvm --include='*/' --exclude='*' src/",
    "copy:cjs": "npm run copy dist",
    "copy:esm": "npm run copy dist/esm",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:staged": "jest --findRelatedTests",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "lint:fix": "prettier-eslint \"$(pwd)/**/*.ts\" --write",
    "prepublishOnly": "npm run build",
    "predeploy": "npm run docs",
    "deploy": "gh-pages -d docs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "npm run test:staged",
      "git add"
    ],
    "*.ts": [
      "npm run lint:fix",
      "npm run test:staged",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ticklepoke/finance-math.git"
  },
  "keywords": [
    "finance",
    "math",
    "javascript"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ticklepoke/finance-math/issues"
  },
  "homepage": "https://ticklepoke.github.io/finance-math/",
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/jest": "^24.9.0",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "babel-jest": "^24.9.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "husky": "^4.0.10",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "npm-run-all": "^4.1.5",
    "prettier-eslint": "^9.0.1",
    "prettier-eslint-cli": "^5.0.0",
    "rollup": "^1.29.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^5.2.0",
    "typedoc": "^0.16.7",
    "typescript": ">=3.2.1 <3.6.0"
  }
}
